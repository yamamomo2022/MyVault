{
  "env": {
    "TF_LOG": "WARN",
    "CLAUDE_CODE_ENABLE_TELEMETRY": "0",
    "BASH_DEFAULT_TIMEOUT_MS": "120000"
  },
  "permissions": {
    "allow": [
      "Bash(ls ./)",
      "Bash(ls ./*)",
      "Bash(cat ./*)",
      "Bash(grep * ./)",
      "Bash(rg * ./)",
      "Bash(find ./)",
      "Bash(tree ./)",
      "Bash(head ./*)",
      "Bash(tail ./*)",
      "Bash(echo *)",
      "Bash(pwd)",
      "Bash(cd ./)",
      "Bash(mkdir ./)",
      "Bash(cp ./* ./)",
      "Bash(mv ./* ./)",
      "Bash(touch ./)",
      "Bash(which *)",
      "Bash(env)",
      "Bash(whoami)",
      "Bash(date)",
      "Read(./**)",
      "Edit(./**)",
      "Grep(./**)",
      "Glob(./**)",
      "LS(./**)",
      "Write(./**)",
      "MultiEdit(./**)",
      "TodoRead(**)",
      "TodoWrite(**)",
      "Task(**)"
    ],
    "deny": [
      "Bash(rm -rf*)",
      "Bash(rm /*)",
      "Bash(cp /* *)",
      "Bash(cp * /*)",
      "Bash(mv /* *)",
      "Bash(mv * /*)",
      "Bash(mkdir /*)",
      "Bash(sudo*)",
      "Write(.git/**)"
    ]
  },
  "enabledMcpjsonServers": [
  ],
  "disabledMcpjsonServers": [],
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Funk.aiff"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Funk.aiff"
          }
        ]
      }
    ]
  }
}